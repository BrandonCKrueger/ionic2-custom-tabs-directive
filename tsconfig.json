{
  "version": "1.5.3",
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "listFiles": true,
    "allowNonTsExtensions": true,
    "experimentalDecorators": true,
    "isolatedModules": true,
    "noEmitOnError": false,
    "rootDir": "."
  },
  "compileOnSave": false
}